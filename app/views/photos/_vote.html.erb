<% if @round && current_user.juror? %>
  <% if @round.votes.where(:user_id => current_user.id, :photo_id => photo.id).empty? %>
    <% vote = photo.votes.build %>
    <div class="ballot">
      <%= form_for vote, :url => round_votes_path(@round), :html => {:class => 'ajax', "data-photo-id" => photo.id} do |f| %>
        <%= f.hidden_field :photo_id  %>
        <%= f.submit "Vote for this photo" %>
      <% end %>
    </div>
  <% else %>
    <div class="completed_vote">Voted</div>
  <% end %>
<% end %>
